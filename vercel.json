{
  "version": 2,
  "builds": [
    {
      "src": "TweetBlog/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "TweetBlog/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_ALLOWED_HOSTS": "localhost 127.0.0.1 TweetBlog.vercel.app"
  }
}
